{
    "name": "garbetjie/laravel-auth-passport-client",
    "description": "Laravel Passport guard that fetches the authenticated OAuth client like a user.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-passport",
        "laravel-auth",
        "guard"
    ],
    "authors": [
        {
            "name": "Geoff Garbers",
            "email": "geoff@garbers.co.za"
        }
    ],
    "autoload": {
        "psr-4": {
            "Garbetjie\\Laravel\\Auth\\PassportClient\\": "src"
        }
    },
    "require": {
        "laravel/passport": "^8.0 || ^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Garbetjie\\Laravel\\Auth\\PassportClient\\PassportServiceProvider"
            ]
        }
    }
}
